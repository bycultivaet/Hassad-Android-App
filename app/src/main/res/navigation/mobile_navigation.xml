<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_poll">

    <fragment
        android:id="@+id/navigation_poll"
        android:name="com.cultivaet.hassad.ui.main.poll.PollFragment"
        android:label="@string/title_poll"
        tools:layout="@layout/fragment_poll" />

    <fragment
        android:id="@+id/navigation_missions"
        android:name="com.cultivaet.hassad.ui.main.missions.MissionsFragment"
        android:label="@string/title_missions"
        tools:layout="@layout/fragment_missions" />

    <fragment
        android:id="@+id/navigation_addition"
        android:name="com.cultivaet.hassad.ui.main.addition.AdditionFragment"
        android:label="@string/title_addition"
        tools:layout="@layout/fragment_addition" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.cultivaet.hassad.ui.main.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>